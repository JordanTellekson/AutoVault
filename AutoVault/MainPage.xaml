<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:AutoVault"
             xmlns:components="clr-namespace:AutoVault.Components"
             x:Class="AutoVault.MainPage"
             Padding="{OnPlatform iOS='0,40,0,0', Android='0,35,0,0', MacCatalyst='0,20,0,0', WinUI='0,32,0,0'}">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type components:Routes}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>